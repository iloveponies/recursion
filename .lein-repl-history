(defn my-frequencies-helper [freqs a-seq]
  (let [key (first a-seq)
        val (get freqs val)]
    (if val
      (my-frequencies-helper (assoc freqs key (inc val)) (rest a-seq))
      (my-frequencies-helper (assoc freqs key 1) (rest a-seq)))                 
    ))
