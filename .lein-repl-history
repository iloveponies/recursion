    ))
(defn my-range [up-to]
  (if (= up-to 0)
    0
    (cons (dec up-to)(my-range (- up-to 2)))
    ))
(my-range 3)
(defn my-range [up-to]
  (cond
    (<= up-to 0) nil
    (= up-to 1) '(0)
    :else (cons (dec up-to)(my-range (- up-to 2)))
    ))
(my-range 3)
