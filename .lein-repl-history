(defn my-frequencies-helper [freqs a-seq]
  (let [key (first a-seq)
        val (get freqs key)
        rem-seq (rest a-seq)]
    (if (empty? a-seq)
      freqs
      (if val
            (my-frequencies-helper (assoc freqs key (inc val)) rem-seq)
            (my-frequencies-helper (assoc freqs key 1) rem-seq)
            ))))
