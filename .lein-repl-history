(rotations #{1 5 3})
(reverse (first (rotations #{1 5 3})))
(defn permutations [a-set]
  (loop [result (rotations a-set)
         to-go (rotations a-set)]
    (if
      (empty? to-go) 
      result
      (recur (conj result (reverse (first to-go))) (rest to-go)))))
