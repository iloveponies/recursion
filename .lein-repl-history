(defn my-take [n coll]
  (if (= n 0)
    coll
    (cons (first coll) (my-take (dec n) (rest coll)))
    ))
(my-take 3 f)
(defn my-take [n coll]
  (if (= n 0)
    nil
    (cons (first coll) (my-take (dec n) (rest coll)))
    ))
(my-take 3 f)
(defn my-drop [n coll]
  (if (= n 0)
    coll
    (my-drop (dec n) (rest coll))
    ))
(my-drop 2 f)
(my-drop 4 f)
