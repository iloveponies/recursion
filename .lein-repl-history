(defn my-frequencies-helper [freqs a-seq]
  (let [key (first a-seq)
        val (get freqs val)]
    (if val
      (my-frequencies-helper (assoc freqs key val) (rest a-seq))
      (my-frequencies-helper freqs (rest a-seq)))                 
    ))
(my-frequencies [:a "moi" :a "moi" "moi" :a 1])
