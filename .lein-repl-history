(defn powerset [a-set]
  (defn powerset-helper [elem set1 acc]
    (if (empty? set1)
      acc
      (conj (conj acc #{elem}) set1)
      ))
  (map (fn [x] (powerset-helper x (disj a-set x) #{a-set})) a-set))
